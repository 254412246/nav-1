ia (min-width: 500px) {\r\n    .siteList > li{\r\n        margin-right: 25px;\r\n    }\r\n\r\n}\r\n\r\n.siteList .site{\r\n    width: 160px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    background: #8f8f8f;\r\n    border: 1px solid #dddddd;\r\n    border-radius: 4px;\r\n    padding: 20px  0px;\r\n    position: relative;\r\n    cursor: pointer;\r\n}\r\n.siteList .site .close{\r\n    position: absolute;\r\n    transform:scale(1.5);\r\n    top: 10px;\r\n    right: 10px;\r\n}\r\n@media (min-width: 500px) {\r\n    .siteList .site .close{\r\n        display: none;\r\n    }\r\n    .siteList .site:hover .close{\r\n        display: block;\r\n    }\r\n}\r\n.siteList .site .logo{\r\n    width: 64px;\r\n    height: 64px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center; \r\n    font-size: 64px;\r\n}\r\n.siteList .site .link{ \r\n    font-size: 14px;\r\n    margin-top: 4px;\r\n}\r\n\r\n.siteList .addButton{\r\n    width: 160px;\r\n    background: #8f8f8f;\r\n    border: 1px solid #dddddd;\r\n    border-radius: 4px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center; \r\n    flex-direction: column;\r\n    padding: 20px 0;\r\n    height: 129px;\r\n}\r\n.siteList .addButton .icon{\r\n    width: 63px;\r\n    height: 63px  \r\n}\r\n.siteList .addButton .text{\r\n    font-size: 14px;\r\n    margin-top: 4px;\r\n    \r\n}\r\n.siteList .addButton .icon-Wrapper{\r\n    width: 64px;\r\n    height: 64px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center; \r\n}"},"lineCount":null}},"error":null,"hash":"3321c2ad462bbaa7bd5689c2aa9ed202","cacheData":{"env":{}}}